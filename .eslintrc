{
  "extends": ["prettier", "prettier/react"],
  "parser": "babel-eslint",
  "plugins": ["react", "react-hooks", "prettier"],
  "env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    "react/jsx-fragments": [0],
    "arrow-parens": 0,
    "import/no-extraneous-dependencies": 0,
    "prettier/prettier": [
      2,
      {
        "printWidth": 100,
        "singleQuote": true,
        "trailingComma": "es5"
      }
    ]
  }
}
